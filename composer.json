{
    "name": "dennisvandalen/shipment-url-parser-php",
    "description": "This is my package shipment-url-parser-php",
    "keywords": [
        "dennisvandalen",
        "shipment-url-parser-php"
    ],
    "homepage": "https://github.com/dennisvandalen/shipment-url-parser-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis van Dalen",
            "email": "hi@vandalen.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "DennisVanDalen\\ShipmentUrlParser\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DennisVanDalen\\ShipmentUrlParser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
